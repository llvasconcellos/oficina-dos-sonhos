	<params>
		<param name="ja_color" type="list" default="default" label="Color Variation" description="Color variation to use">
		  <option value="default">Default</option>	 
		  <option value="green">Green</option>
		  <option value="blue">Blue</option>
		  </param>
		<param name="ja_font" type="list" default="3" label="Font size" description="Font size to use">
		  <option value="1">1</option>
		  <option value="2">2</option>
		  <option value="3">3</option>
		  <option value="4">4</option>
		  <option value="5">5</option>
		  <option value="6">6</option>
		  </param>
		<param name="ja_menu" type="list" default="5" label="Menu Module" description="Menu display module">
		  <option value="1">Split Menu</option>
		  <option value="2">Suckerfish Menu</option>
		  <option value="5">Transmenu</option>	    
		  </param>
		<param name="usertool" type="list" default="6" label="Display template tool" description="Display template tool">
		  <option value="0">No display</option>
		  <option value="1">Screen Type</option>
		  <option value="2">Font Size</option>
		  <option value="4">Color Css</option>
		  <option value="3">Screen Type and Font Size</option>
		  <option value="5">Screen Type and Color Css</option>
		  <option value="6">Font site and Color Css</option>
		  <option value="7">All tools</option>
		</param>
		<param name="ja_screen" type="list" default="wide" label="Template screen" description="Template screen">
		  <option value="wide">Wide</option>
		  <option value="narrow">Narrow</option>
		  <option value="auto">Auto</option>		
		</param>
		<param name="ja_iconmenu" type="radio" default="1" label="Template Icon menu" description="Template Icon menu">
		  <option value="0">No</option>
		  <option value="1">Yes</option>
		</param>
  </params>
